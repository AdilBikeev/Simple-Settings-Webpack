    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "csv-loader": "^3.0.3",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.0.0",
